@model ShibpurConnectWebApp.Models.WebAPI.EmploymentHistories
           
<div id="employmentDetails">
    <div class="form-group">
        @Html.LabelFor(m => m.CompanyName, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.CompanyName, new { @class = "form-control", @placeholder = "CompanyName" })
        </div>
    </div>
    
    <div class="form-group">
        @Html.LabelFor(m => m.From, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.From, new { @class = "form-control", @placeholder = "From" })
        </div>
    </div>
    
    <div class="form-group">
        @Html.LabelFor(m => m.To, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.To, new { @class = "form-control", @placeholder = "To" })
        </div>
    </div>
    
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-primary" value="Save" onclick="saveEmploymentHistory(); return false;" />
        </div>

    </div>
</div>