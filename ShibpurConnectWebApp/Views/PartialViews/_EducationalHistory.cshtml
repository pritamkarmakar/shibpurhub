@model ShibpurConnectWebApp.Models.WebAPI.EducationalHistories

<div id="educationalHistory">
    <form id="loginForm" method="post" class="form-horizontal">
        <div class="form-group">
            @Html.LabelFor(m => m.UniversityName, new { @class = "col-md-2 control-label" })
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.UniversityName, new { @class = "form-control", @placeholder = "UniversityName" })
            </div>
        </div>

        @Html.Partial("../PartialViews/_DepartmentDropdown")

        <div class="form-group">
            @Html.LabelFor(m => m.GraduateYear, new { @class = "col-md-2 control-label" })
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.GraduateYear, new { @class = "form-control", @placeholder = "Passout Year/Expected Passout Year" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" class="btn btn-primary btn-sm" value="Save" onclick="saveEducationalHistory(); return false;" />
            </div>

        </div>
    </form>
</div>